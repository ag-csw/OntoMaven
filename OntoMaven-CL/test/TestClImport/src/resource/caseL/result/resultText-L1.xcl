<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="../../xcl2.rnc" type="application/relax-ng-compact-syntax"?>
<Construct xmlns="http://purl.org/xcl/2.0/">
  <Titling>
    <Name cri="http://example.org/caseL/myText-L1#a"/>
    <Construct>
      <Titling>
        <Comment>This nested titling only comes into play when its outer titling is imported.
        </Comment>
        <Name cri="http://example.org/caseL/myText-L1#c"/>
        <Construct>
          <Atom><Name>V</Name><Name>w</Name></Atom>
        </Construct>
      </Titling>  
      <Atom><Name>P</Name><Name>a</Name></Atom>
    </Construct>
  </Titling>          
</Construct>
